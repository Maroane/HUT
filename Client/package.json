{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@storybook/addons": "^3.2.18",
    "autoprefixer": "7.1.6",
    "axios": "^0.17.1",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "color": "^2.0.1",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "^4.13.1",
    "eslint-config-react-app": "^2.0.1",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "file-saver": "^1.3.3",
    "fs-extra": "3.0.1",
    "gsap": "^1.20.3",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "mo-js": "^0.288.2",
    "moment": "^2.19.4",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.2.0",
    "react-gsap-enhancer": "^0.3.0",
    "react-portal": "^4.1.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.2.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "storybook": "^1.0.0",
    "style-loader": "0.19.0",
    "styled-jsx": "^2.2.1",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "precommit": "jest --verbose && npm lint",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "node scripts/test.js --env=jsdom",
    "standard": "standard --verbose | snazzy",
    "standard-fix": "standard --fix --parser babel-eslint | snazzy",
    "lint": "eslint --ext .js --ext .jsx src --fix",
    "format": "prettier-standard '**/*.js' | standard --fix --parser babel-eslint | snazzy"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "context",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "expect"
    ],
    "ignore": [
      "build/",
      "config/",
      "src/registerServiceWorker.js"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      "stage-3"
    ],
    "plugins": [
      "transform-class-properties",
      "styled-jsx/babel"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "standard"
    ],
    "parser": "babel-eslint",
    "env": {
      "es6": true,
      "mocha": true
    },
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    }
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^3.2.10",
    "@storybook/addon-storyshots": "^3.2.11",
    "@storybook/react": "^3.2.17",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-stage-3": "^6.24.1",
    "husky": "^0.14.3",
    "prettier-standard": "^7.0.3",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3"
  }
}
